<div class="pull-right">
  <%= link_to("Add a job", new_job_path, :class => "btn btn-default")%>
</div>

<table class="table table-boldered">
  <% @jobs.each do |j| %>
    <tr>
      <td><%= link_to(j.title, job_path(j) )%></td>
      <td>
      <%= link_to("Edit", edit_job_path(j)) %> |
      <%= link_to("Destroy", job_path(j), :method => :delete, :data => { :confirm => "Are you sure?" }) %>
      </td>
      <td><%=j.created_at%></td>
    </tr>
    <% end %>
</table>
